# Coze 工作流配置
# 在 Vercel 部署时，需要在 Project Settings > Environment Variables 中配置

COZE_API_ENDPOINT=https://your-workflow.coze.site/run
COZE_API_TOKEN=your_coze_api_token_here

# DeepSeek API 配置（用于智能引导功能）
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# 管理后台配置
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password
AUTH_SECRET=random_32_char_string_for_encryption
